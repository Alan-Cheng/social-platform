{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      loginMessage: '' // 用於顯示登入結果消息\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8787/login', {\n          username: this.username,\n          password: this.password\n        });\n        if (response.status === 200) {\n          // 登入成功，設定登入消息\n          this.$router.push({\n            path: '/SocialMedia',\n            query: {\n              userId: response.data.userId\n            }\n          });\n          this.loginMessage = '登入成功';\n          // 触发登录成功事件\n          this.$emit('loginSuccess');\n        }\n      } catch (error) {\n        console.error(error);\n        if (error.response) {\n          // 根據後端返回的狀態碼顯示具體的錯誤訊息\n          if (error.response.status === 404) {\n            this.loginMessage = \"帳號不存在\";\n          } else if (error.response.status === 401) {\n            this.loginMessage = \"密碼錯誤\";\n          } else {\n            this.loginMessage = \"登入失敗，請稍後再試\";\n          }\n        } else {\n          this.loginMessage = \"登入失敗，請檢查網絡連接或稍後再試\";\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","loginMessage","methods","login","response","post","status","$router","push","path","query","userId","$emit","error","console"],"sources":["/Users/alan/Desktop/social-platform/frontend/src/components/LogIn.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>已有帳號請登入</h2>\n    <form @submit.prevent=\"login\">\n      <label for=\"username\">帳號:</label>\n      <input type=\"text\" id=\"username\" v-model=\"username\" maxlength=\"10\" placeholder=\"帳號為您的手機號碼\" required>\n      <label for=\"password\">密碼:</label>\n      <input type=\"password\" id=\"password\" v-model=\"password\" required>\n      <button type=\"submit\">登入</button>\n    </form>\n    <!-- 顯示登入結果消息 -->\n    <div v-if=\"loginMessage\">{{ loginMessage }}</div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      loginMessage: '' // 用於顯示登入結果消息\n    }\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:8787/login', {\n          username: this.username,\n          password: this.password\n        });\n\n        if (response.status === 200) {\n          // 登入成功，設定登入消息\n          this.$router.push({ path: '/SocialMedia', query: { userId: response.data.userId } });\n          this.loginMessage = '登入成功';\n          // 触发登录成功事件\n          this.$emit('loginSuccess');\n        }\n      } catch (error) {\n        console.error(error);\n        if (error.response) {\n          // 根據後端返回的狀態碼顯示具體的錯誤訊息\n          if (error.response.status === 404) {\n            this.loginMessage = \"帳號不存在\";\n          } else if (error.response.status === 401) {\n            this.loginMessage = \"密碼錯誤\";\n          } else {\n            this.loginMessage = \"登入失敗，請稍後再試\";\n          }\n        } else {\n          this.loginMessage = \"登入失敗，請檢查網絡連接或稍後再試\";\n        }\n      }\n    }\n  }\n}\n</script>\n"],"mappings":";AAgBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAC,CAAE;IACnB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,6BAA6B,EAAE;UAC/DN,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAII,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3B;UACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YAAEC,IAAI,EAAE,cAAc;YAAEC,KAAK,EAAE;cAAEC,MAAM,EAAEP,QAAQ,CAACN,IAAI,CAACa;YAAO;UAAE,CAAC,CAAC;UACpF,IAAI,CAACV,YAAW,GAAI,MAAM;UAC1B;UACA,IAAI,CAACW,KAAK,CAAC,cAAc,CAAC;QAC5B;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpB,IAAIA,KAAK,CAACT,QAAQ,EAAE;UAClB;U